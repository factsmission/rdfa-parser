{
  "name": "@factsmission/rdfa-parser",
  "version": "0.0.3",
  "description": "A parser for RDFa",
  "main": "lib/index.js",
  "repository": "https://github.com/factsmission/rdfa-parser",
  "author": "Noam Bachmann",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prepublish": "./node_modules/.bin/babel src --out-dir lib",
