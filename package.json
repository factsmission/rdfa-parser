    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "cross-var": "^1.1.0",
    "express": "^4.16.3",
    "fetch": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "http": "^0.0.0",
    "mocha": "^5.2.0",
    "ncp": "^2.0.0",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  }
